{
	"compilerOptions": {
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"strict": true,
		"resolveJsonModule": true,
		"module": "commonjs",
		"target": "es2017",
		"outDir": "dist",
		"rootDir": "src",
		"lib": [
			"es2017"
		],
		"sourceMap": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}